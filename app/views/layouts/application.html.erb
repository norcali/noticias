<!DOCTYPE html>
<html>
<head>
  <title>Agregador</title>
  <%= stylesheet_link_tag "reset", "backend", "responsive", "style", "fontface", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
<% end %>

<% if user_signed_in? %>
  <% #link_to "My Profile", profiles_dashboard_path%>
  <% #link_to('Logout', destroy_user_session_path, :method=>'delete', :class => "add") %>
<%end%>

</body>
</html>

